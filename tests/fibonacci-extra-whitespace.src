!{#include <stdio.h>}!

    
     
       

external fn printf(text: String, x: I32)
 
  
  

fn fib(x: I32, y: I32, n: I32): I32
 
  if(n > 0)
   
    var z = x + y



    return fib(y, z, n - 1)


  else


    return x
    
fn main()
     

  for(var i = 0; i < 12; i = i + 1)
          

    printf("%d\n", fib(0, 1, i))
 
 
  
